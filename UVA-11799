#include<stdio.h>

int main()
{

    int a[100],i,j,t,p,k,m,test;

   scanf("%d",&test);

   for(p=1; p<=test; p++){

    scanf("%d",&k);

    for(i=0; i<k; i++){

        scanf("%d",&a[i]);


    }
       t=0;
       t=a[0];

    for(j=0; j<k; j++){

        if(t<a[j]){

            t=a[j];
        }
    }

    printf("Case %d: %d\n",p,t);
   }
    return 0;
}
