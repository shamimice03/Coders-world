#include<stdio.h>
#include<string.h>

int main()
{
    int p=0,t;
    int a;
    int num=0;
        scanf("%d",&t);
        while(t--){
    scanf("%d",&a);

    num=a*567;
    num=num/9;
    num=num+7492;
    num=num*235;
    num=num/47;
    num=num-498;


    p=(num/10)%10;
   if(num<0){
   p=-p;
   }
    printf("%d\n",p);
        }
    return 0;
}
