#include<stdio.h>
#include<string.h>

int main()
{
    char word[5];
    int i,j,k,len=0;

scanf("%d%*c",&k);

while(k--){

    gets(word);
    len=strlen(word);
    if(len==5){
        printf("3\n");
    }
    else if(len==3){

        if(word[0]=='o' && word[1]=='n'){
            printf("1\n");
        }
        else if(word[0]=='o' && word[2]=='e'){
            printf("1\n");
        }
        else if(word[1]=='n' && word[2]=='e'){
            printf("1\n");
        }
           else{
        printf("2\n");
    }
    }

}
    return 0;
}
///End of source code.
