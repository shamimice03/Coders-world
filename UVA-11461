#include<stdio.h>
#include<math.h>

int main()
{
    int start,end;
    float b;
    int i,count=0;

    while(scanf("%d %d",&start,&end) != EOF){
            if(start==0 && end==0){
                break;
            }

          count=0;
    for(i=start; i<=end; i++){
                 b=(float)sqrt(i);

            if( ((float)b-(int)b)==0 ){
                count++;
            }
        }
            printf("%d\n",count);


    }


    return 0;
}
